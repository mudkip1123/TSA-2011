class ROAD extends MovieClip {	var scrollSpeed;	var spawnTimer;	var enemies;	//purple	var bonusSpawnTimer;	//coin	//related to enemy spawning	function onLoad() {		scrollSpeed = 10;		spawnTimer = 0;		enemies = [];		//purple		bonusSpawnTimer = 0;		//coin	}	function onEnterFrame() {		spawnTimer += 1;		bonusSpawnTimer += 1;		//counts each frame		this._y += scrollSpeed;		//scrolling		if (_y>699) {			_y = 233;		}		if (spawnTimer>20) {			spawnTimer = 0;			var Enemy = _root.attachMovie("Enemy", "Enemy"+_root.getNextHighestDepth(), _root.getNextHighestDepth());			enemies.push(Enemy);		}	}}