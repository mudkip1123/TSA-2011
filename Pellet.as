class Pellet extends MovieClip{	var speed;		function onLoad()	{		speed = 8;			}		function onEnterFrame()	{		_y += speed;		if (_y > 500)		{			this.removeMovieClip()		}				for ( var i in _root.Master.enemies)		{			if (this.hitTest(_root.Master.enemies[i]))			{				var Explosion = _root.attachMovie("Explosion", "Explosion"+_root.getNextHighestDepth(), _root.getNextHighestDepth());				Explosion._x = _x;				Explosion._y = _y;				this.removeMovieClip()				_root.Master.enemies[i].removeMovieClip()				_root.Master.updateScore(2);							}		}			}}