class Pellet extends MovieClip{	var speed;	var despawnCounter;		function onLoad()	{		speed = 5;		despawnCounter = 0;			}		function onEnterFrame()	{		_y += speed;		despawnCounter += 1;		if (_y <-20)		{			this.removeMovieClip()		}				for ( var i in _root.GameMaster.enemies)		{			if (this.hitTest(_root.GameMaster.enemies[i]))			{				var Explosion = _root.attachMovie("Explosion", "Explosion"+_root.getNextHighestDepth(), _root.getNextHighestDepth());				Explosion._x = _x;				Explosion._y = _y;				this.removeMovieClip()				_root.GameMaster.enemies[i].removeMovieClip()				_root.Car.updateScore(2);							}		}				if (despawnCounter > 10) {			this.removeMovieClip()		}	}}